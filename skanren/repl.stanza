
defpackage repl :
    import core
    import reader
    import skanren


println("Hello there!  Welcome to the repl!")

val universe = Universe()

while true:
    print("(\\\")> ")
    val form = unwrap-all(read-line(STANDARD-INPUT-STREAM))
    for s-exp in form do:
        eval(universe, s-exp)
    println("length(universe) = %_" % [length(universe)])
    println("universe = [%,]" % [universe])
